<p id="notice"><%= notice %></p>

<h1>Stores</h1>

<table>
  <thead>
    <tr>
      <th>Store name</th>
      <th>Store address</th>
      <th>Food category</th>
      <th>Min price</th>
      <th>Tip</th>
      <th>Content</th>
      <th>Guide</th>
      <th>Phone</th>
      <th>Delivery address</th>
      <th>Operation address</th>
      <th>Closeddays</th>
      <th>Dibs count</th>
      <th>Review count</th>
      <th>Rating count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stores.each do |store| %>
      <tr>
        <td><%= store.store_name %></td>
        <td><%= store.store_address %></td>
        <td><%= store.food_category %></td>
        <td><%= store.min_price %></td>
        <td><%= store.tip %></td>
        <td><%= store.content %></td>
        <td><%= store.guide %></td>
        <td><%= store.phone %></td>
        <td><%= store.delivery_address %></td>
        <td><%= store.operation_address %></td>
        <td><%= store.closedDays %></td>
        <td><%= store.dibs_count %></td>
        <td><%= store.review_count %></td>
        <td><%= store.rating_count %></td>
        <td><%= link_to 'Show', store %></td>
        <td><%= link_to 'Edit', edit_store_path(store) %></td>
        <td><%= link_to 'Destroy', store, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store', new_store_path %>

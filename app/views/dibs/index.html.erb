<p id="notice"><%= notice %></p>

<h1>Dibs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dibs.each do |dib| %>
      <tr>
        <td><%= link_to 'Show', dib %></td>
        <td><%= link_to 'Edit', edit_dib_path(dib) %></td>
        <td><%= link_to 'Destroy', dib, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dib', new_dib_path %>
